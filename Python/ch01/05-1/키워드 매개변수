#             print(word)
#         print()

# print_n_times('안녕하세요','즐거운','파이썬 프로그래밍')

def print_n_times(*values,n=2): #기본 매개변수는 가변 매개변수 뒤로
    for i in range(n):
        for word in values:
            print(word)
        print()

print_n_times('안녕하세요','즐거운','파이썬 프로그래밍')
