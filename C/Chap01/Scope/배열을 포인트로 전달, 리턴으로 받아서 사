#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

char* func1(char a) {
	return &a;
}

int* func2(int* pa) {
	return pa;
}

int main()
{
	char a = 'Y';
	int arr[3] = { 1,2,3 };
	int* pb;

	pb = func2(arr);

	for (int i = 0; i < 3; i++) {
		printf("%d\n", pb[i]);
	}
	
	printf("%p\n", func1);
	return 0;
}
