#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <stdlib.h>
#include <time.h>//1970.01.01시 기준

int main()
{
	srand((unsigned int)time(NULL));//random의 초기값을 잡음(seed값이 고정이면 항상 동일한 난수가 나옴)

	for (int i = 0; i < 6; i++) {
		int num = rand() % 45 + 1; //0~44 + 1 => 1~45까지
		printf("난수 : %d\n", num);
	}
	return 0;
}
