-구체화-
#include <iostream>
#include <cstdlib>
#include <memory>
using namespace std;

template <class T>
void myswap(T & a, T & b){
    T tmp;
    tmp = a;
    a = b;
    b = tmp;
}

int main()
{
    int a = 4, b = 5;
    myswap(a,b);

    cout << "a : " << a << " b : " << b << endl;
    return 0;
}
